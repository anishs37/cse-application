{
  "name": "cse-electron",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ShoppingList --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
    "package-linux": "electron-packager . ShoppingList --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "rebuild-robojs": "electron-rebuild -f -w robojs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.9",
    "@tensorflow/tfjs": "^3.15.0",
    "deepspeech": "^0.9.3",
    "deepspeech-tflite": "^0.9.3",
    "electron": "^18.0.1",
    "mic": "^2.1.2",
    "node-gyp": "^9.0.0",
    "node-vad": "^1.1.4",
    "robotjs": "^0.6.0",
    "speaker": "^0.5.4",
    "uuid": "^8.3.2",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "electron-rebuild": "^3.2.7"
  }
}
